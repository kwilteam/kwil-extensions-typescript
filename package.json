{
  "name": "@kwilteam/extensions",
  "version": "0.0.4",
  "description": "A JS / TS SDK for building Kwil Extensions.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*.js",
    "dist/**/*.ts",
    "!dist/**/*.map",
    "!dist/**/*.test.js",
    "!dist/**/*.test.ts"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "husky install",
    "commit": "git add . && cz",
    "release": "standard-version"
  },
  "keywords": [],
  "author": "Luke Lamey (luke@kwil.com)",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/google-protobuf": "^3.15.6",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "standard-version": "^9.5.0",
    "ts-proto": "^1.152.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.17",
    "google-protobuf": "^3.21.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwilteam/kwil-extensions-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/kwilteam/kwil-extensions-typescript/issues"
  },
  "homepage": "https://github.com/kwilteam/kwil-extensions-typescript#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
